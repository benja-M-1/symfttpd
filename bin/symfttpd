error_reporting(E_ALL|E_STRICT);
ini_set('display_errors', true);
set_time_limit(0);

if (file_exists($f = __DIR__ . '/../../../autoload.php')) {
    require_once $f;
} else {
    require_once __DIR__ . '/../vendor/autoload.php';
}

$application = new Symfttpd\Console\Application();
$application->run();
