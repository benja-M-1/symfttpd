{
    "name": "benjam1/symfttpd",
    "description": "PHP webserver",
    "keywords": ["symfony", "server", "webserver", "lighttpd"],
    "type": "library",
    "licence": "MIT",
    "authors": [
        {
            "name":     "Benjamin Grandfond",
            "email":    "benjamin.grandfond@gmail.com",
            "homepage": "http://benjamingrandfond.fr"
        },
        {
            "name":     "Laurent Bachelier",
            "email":    "laurent@bachelier.name",
            "homepage": "http://laurent.bachelier.name"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/console":    "2.2.*",
        "symfony/process":    "2.2.*",
        "symfony/finder":     "2.2.*",
        "symfony/filesystem": "2.2.*",
        "symfony/config":     "2.2.*",
        "twig/twig":          ">=1.10,<2.0-dev"
    },
    "autoload": {
        "psr-0": {
            "Symfttpd": "lib/"
        }
    },
    "bin": ["bin/symfttpd"],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.0"
        }
    },
    "minimum-stability": "dev"
}
