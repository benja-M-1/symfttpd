{
    "name": "laurentb/symfttpd",
    "description": "Symfony webserver",
    "keywords": [
        "symfony",
        "server",
        "webserver"
    ],
    "type": "project",
    "authors": [
        {
            "name":     "Laurent Bachelier",
            "email":    "laurent@bachelier.name",
            "homepage": "http://laurent.bachelier.name"
        },
        {
            "name":     "Benjamin Grandfond",
            "email":    "benjamin.grandfond@gmail.com",
            "homepage": "http://benjamingrandfond.fr"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/console":    "dev-master",
        "symfony/process":    "dev-master",
        "symfony/filesystem": "dev-master",
        "kriswallsmith/spork": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "kriswallsmith/spork",
                "source": {
                    "url": "https://github.com/kriswallsmith/spork.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "Spork": "src/" }
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "Symfttpd": "lib/"
        }
    },
    "bin": [
        "bin/symfttpd"
    ]
}