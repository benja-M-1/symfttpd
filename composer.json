{
    "name": "benjam1/symfttpd",
    "description": "PHP webserver",
    "keywords": ["symfony", "server", "webserver", "lighttpd", "nginx", "php-fpm"],
    "type": "library",
    "licence": "MIT",
    "authors": [
        {
            "name":     "Benjamin Grandfond",
            "email":    "benjamin.grandfond@gmail.com",
            "homepage": "http://benjamingrandfond.fr"
        },
        {
            "name":     "Laurent Bachelier",
            "email":    "laurent@bachelier.name",
            "homepage": "http://laurent.bachelier.name"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/console":    ">=2.1, <2.3-dev",
        "symfony/process":    ">=2.1, <2.3-dev",
        "symfony/finder":     ">=2.1, <2.3-dev",
        "symfony/filesystem": ">=2.1, <2.3-dev",
        "symfony/config":     ">=2.1, <2.3-dev",
        "twig/twig":          ">=1.10,<2.0-dev",
        "pimple/pimple":      "1.0.*@dev",
        "monolog/monolog":    "1.4.*@dev"
    },
    "autoload": {
        "psr-0": {
            "Symfttpd": "src/"
        }
    },
    "bin": ["bin/symfttpd"],
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "minimum-stability": "dev"
}
